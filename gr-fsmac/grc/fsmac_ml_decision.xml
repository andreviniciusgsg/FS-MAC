<?xml version="1.0"?>
<block>
  <name>Mach. Learning Decision</name>
  <key>fsmac_ml_decision</key>
  <category>[FS-MAC 2.0]</category>
  <import>import fsmac</import>
  <make>fsmac.ml_decision($ml_model, $is_coord, $alpha, $filename, $training_file, $aggr1, $aggr2, $aggr3, $aggr4, $aggr5, $aggr_max, $periodicity)</make>
  <!-- Make one 'param' node for every Parameter you want settable from the GUI.
       Sub-nodes:
       * name
       * key (makes the value accessible as $keyname, e.g. in the make node)
       * type -->

  <param>
    <name>Mach. Learning model</name>
    <key>ml_model</key>
    <value>2</value>
    <type>int</type>

    <option>
      <name>None, CSMA</name>
      <key>0</key>
    </option>

    <option>
      <name>None, TDMA</name>
      <key>1</key>
    </option>

    <option>
      <name>FS-MAC</name>
      <key>2</key>
    </option>

    <option>
      <name>Linear</name>
      <key>3</key>
    </option>

    <option>
      <name>Linear SVM</name>
      <key>4</key>
    </option>

    <option>
      <name>SVM</name>
      <key>5</key>
    </option>

    <option>
      <name>NuSVM</name>
      <key>6</key>
    </option>

    <option>
      <name>KNeighbors Reg.</name>
      <key>7</key>
    </option>

    <option>
      <name>Decision Tree</name>
      <key>8</key>
    </option>

    <option>
      <name>Neural Net. Reg.</name>
      <key>9</key>
    </option>

    <option>
      <name>Gradient Boosting Reg.</name>
      <key>10</key>
    </option>
  </param>

  <param>
    <name>Type</name>
    <key>is_coord</key>
    <value>False</value>
    <type>bool</type>

    <option>
      <name>Coordinator</name>
      <key>True</key>
    </option>
    <option>
      <name>Normal</name>
      <key>False</key>
    </option>
  </param>

  <param>
    <name>Alpha</name>
    <key>alpha</key>
    <value>0.1</value>
    <type>float</type>
  </param>

  <param>
    <name>Log file</name>
    <key>filename</key>
    <type>file_save</type>
  </param>

  <param>
    <name>Training file</name>
    <key>training_file</key>
    <type>file_open</type>
  </param>

  <param>
    <name>Aggr sensor 1</name>
    <key>aggr1</key>
    <value>3</value>
    <type>int</type>
    <option>
      <name>None</name>
      <key>0</key>
    </option>
    <option>
      <name>Mov. Avg.</name>
      <key>1</key>
    </option>
    <option>
      <name>Sum</name>
      <key>2</key>
    </option>
    <option>
      <name>Max</name>
      <key>3</key>
    </option>
    <option>
      <name>Min</name>
      <key>4</key>
    </option>
    <option>
      <name>Simple avg</name>
      <key>5</key>
    </option>
  </param>

  <param>
    <name>Aggr sensor 2</name>
    <key>aggr2</key>
    <value>1</value>
    <type>int</type>
    <option>
      <name>None</name>
      <key>0</key>
    </option>
    <option>
      <name>Mov. Avg.</name>
      <key>1</key>
    </option>
    <option>
      <name>Sum</name>
      <key>2</key>
    </option>
    <option>
      <name>Max</name>
      <key>3</key>
    </option>
    <option>
      <name>Min</name>
      <key>4</key>
    </option>
    <option>
      <name>Simple avg</name>
      <key>5</key>
    </option>
  </param>

  <param>
    <name>Aggr sensor 3</name>
    <key>aggr3</key>
    <value>3</value>
    <type>int</type>
    <option>
      <name>None</name>
      <key>0</key>
    </option>
    <option>
      <name>Mov. Avg.</name>
      <key>1</key>
    </option>
    <option>
      <name>Sum</name>
      <key>2</key>
    </option>
    <option>
      <name>Max</name>
      <key>3</key>
    </option>
    <option>
      <name>Min</name>
      <key>4</key>
    </option>
    <option>
      <name>Simple avg</name>
      <key>5</key>
    </option>
  </param>

  <param>
    <name>Aggr sensor 4</name>
    <key>aggr4</key>
    <value>4</value>
    <type>int</type>
    <option>
      <name>None</name>
      <key>0</key>
    </option>
    <option>
      <name>Mov. Avg.</name>
      <key>1</key>
    </option>
    <option>
      <name>Sum</name>
      <key>2</key>
    </option>
    <option>
      <name>Max</name>
      <key>3</key>
    </option>
    <option>
      <name>Min</name>
      <key>4</key>
    </option>
    <option>
      <name>Simple avg</name>
      <key>5</key>
    </option>
  </param>

  <param>
    <name>Aggr sensor 5</name>
    <key>aggr5</key>
    <value>0</value>
    <type>int</type>
    <option>
      <name>None</name>
      <key>0</key>
    </option>
    <option>
      <name>Mov. Avg.</name>
      <key>1</key>
    </option>
    <option>
      <name>Sum</name>
      <key>2</key>
    </option>
    <option>
      <name>Max</name>
      <key>3</key>
    </option>
    <option>
      <name>Min</name>
      <key>4</key>
    </option>
    <option>
      <name>Simple avg</name>
      <key>5</key>
    </option>
  </param>

  <param>
    <name>Aggr max</name>
    <key>aggr_max</key>
    <value>1</value>
    <type>int</type>
    <option>
      <name>None</name>
      <key>0</key>
    </option>
    <option>
      <name>Mov. Avg.</name>
      <key>1</key>
    </option>
    <option>
      <name>Sum</name>
      <key>2</key>
    </option>
    <option>
      <name>Max</name>
      <key>3</key>
    </option>
    <option>
      <name>Min</name>
      <key>4</key>
    </option>
    <option>
      <name>Simple avg</name>
      <key>5</key>
    </option>
  </param>

  <param>
    <name>Periodicity (sec)</name>
    <key>periodicity</key>
    <value>20</value>
    <type>int</type>
  </param>

  <!-- Make one 'sink' node per input. Sub-nodes:
       * name (an identifier for the GUI)
       * type
       * vlen
       * optional (set to 1 for optional inputs) -->

  <sink>
    <name>sensor 1 in</name>
    <type>message</type>
    <optional>1</optional>
  </sink>

  <sink>
    <name>sensor 2 in</name>
    <type>message</type>
    <optional>1</optional>
  </sink>

  <sink>
    <name>sensor 3 in</name>
    <type>message</type>
    <optional>1</optional>
  </sink>

  <sink>
    <name>sensor 4 in</name>
    <type>message</type>
    <optional>1</optional>
  </sink>

  <sink>
    <name>sensor 5 in</name>
    <type>message</type>
    <optional>1</optional>
  </sink>

  <sink>
    <name>max in</name>
    <type>message</type>
    <optional>1</optional>
  </sink>

  <sink>
    <name>act protocol in</name>
    <type>message</type>
    <optional>1</optional>
  </sink>

  <!-- Make one 'source' node per output. Sub-nodes:
       * name (an identifier for the GUI)
       * type
       * vlen
       * optional (set to 1 for optional inputs) -->
  <source>
    <name>out</name>
    <type>message</type>
    <optional>1</optional>
  </source>

  <doc>
    Alpha is a parameter for the moving average aggregation. This aims a smooth pick up of best MAC protocol.

    In respect to the Machine Learning models, visit www.scikit-learn.org for more details. There you can find more information about subtleties of each model.
  </doc>
</block>
